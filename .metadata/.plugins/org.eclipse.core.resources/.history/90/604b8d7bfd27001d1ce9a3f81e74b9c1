package estruturasControle;

import java.util.Scanner;

public class DesafioWhile {

	public static void main(String[] args) {
		
		
		Scanner entrada= new Scanner(System.in);
		
		double nota = entrada.nextDouble();	
		double media = 0;

		while(nota <= 10 || nota >= 0) {
			media += nota;
		}
		
		entrada.close();
	
	}
}
