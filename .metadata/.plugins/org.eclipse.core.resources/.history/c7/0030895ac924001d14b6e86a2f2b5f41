package fundamentos;

public class PrimeiroPrograma {
	
	public static void main(String[] args) {
		
		// Uma sentença de código termina com ';'
		// Testando Novamente
		System.out.println("Uma vez Flamengo, Sempre Flamengo!!!");
		System.out.println("Testando novamente!!!");
		
		
		/*
		 * 
		 * 
		 * Linha 1
		 * 
		 * Linha 2
		 * 
		 * 
		 */
		
		System.out.println("Fim!!"); //Funcionando
	}
}
