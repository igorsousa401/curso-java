package fundamentos;

import java.util.Scanner;

public class DesafioDeConversao {
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);
		
		System.out.println("Digite o antepenúltimo Salário");
		String s1 = entrada.nextLine();
		
		System.out.println("Digite o penúltimo Salário");
		String s2 = entrada.nextLine();
		
		System.out.println("Digite o último Salário");
		String s3 = entrada.nextLine();
		
		Double salario1 = Double.parseDouble(s1);
		Double salario2 = Double.parseDouble(s2);
		Double salario3 = Double.parseDouble(s3);
		
		double media = (salario1 + salario2 + salario3) / 3.0;
		
	}
}
