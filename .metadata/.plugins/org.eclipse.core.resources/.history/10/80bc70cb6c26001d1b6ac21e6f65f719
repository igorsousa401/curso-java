package fundamentos.operadores;

public class OperadoresLogicos {
	public static void main(String[] args) {
		
		boolean c1 = true;
		boolean c2 = 7 > 5;
		
		System.out.println("Teste:");
		System.out.println(c1 && c2);
		System.out.println(c1 || c2);
		System.out.println(c1 ^ c2);
		System.out.println(!c1;
		System.out.println(!!c1);
		
		// Operador LÃ³gico E
		
	}
}
