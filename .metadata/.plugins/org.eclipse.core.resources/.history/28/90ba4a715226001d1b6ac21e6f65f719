package fundamentos;

public class Wrappers {
	public static void main(String[] args) {
		// Byte
		Byte b = 100;
		Short s = 1000;
		System.out.println(b);
		System.out.println(s);
		
		Integer i = Integer.parseInt("10000");
		System.out.println(i);
		
		Long l = 100000L;
		System.out.println(l);
	}
}
